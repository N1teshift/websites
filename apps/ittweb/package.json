{
  "name": "@websites/ittweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "extract:data": "python ./src/features/infrastructure/extraction/scripts/current/manage_extraction.py pipeline",
    "dev": "node scripts/utils/filter-dev-logs.js next dev",
    "dev:verbose": "next dev",
    "type-check": "tsc --noEmit && tsc -p scripts/replay-metadata-parser/tsconfig.json --noEmit",
    "validate-env": "node scripts/utils/validate-env.js",
    "build": "pnpm run validate-env && pnpm --loglevel=error run build:internal",
    "build:internal": "next build",
    "build:quiet": "next build",
    "build:check": "pnpm run type-check && pnpm run build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint:test": "eslint --config config/eslint.test.config.js \"**/__tests__/**/*.{ts,tsx}\" \"**/*.{test,spec}.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testNamePattern=\"(unit|Unit)\" --coverage --coverageDirectory=coverage/unit",
    "test:integration": "jest --testNamePattern=\"(integration|Integration)\" --coverage --coverageDirectory=coverage/integration",
    "test:api": "jest --testPathPattern=\"__tests__/infrastructure/api\" --coverage --coverageDirectory=coverage/api",
    "test:components": "jest --testPathPattern=\"__tests__/modules\" --coverage --coverageDirectory=coverage/components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:mutation": "stryker run",
    "test:mutation:ci": "stryker run --reporters json,dots",
    "analyze:coverage": "node scripts/analyze-coverage.js",
    "analyze:coverage:detailed": "node scripts/analyze-coverage.js --detailed",
    "test:validate": "node scripts/test-validation.js",
    "test:validate:quick": "node scripts/test-validation.js --quick",
    "test:validate:ci": "node scripts/test-validation.js --ci",
    "analyze:replay": "node scripts/utils/analyze-replay.mjs",
    "parse:replay": "node scripts/parse-replay.mjs",
    "parse:replay:all": "node scripts/parse-replay.mjs --all",
    "parse:replay:verbose": "node scripts/parse-replay.mjs --verbose",
    "replay-meta:build": "tsc -p scripts/replay-metadata-parser/tsconfig.json",
    "replay-meta:watch": "tsc -p scripts/replay-metadata-parser/tsconfig.json --watch",
    "replay-meta:decode": "node ./scripts/replay-metadata-parser/dist/cli.js decode"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/formidable": "^3.4.6",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.6.0",
    "formidable": "^3.5.4",
    "gray-matter": "^4.0.3",
    "next": "15.5.7",
    "next-auth": "^4.24.12",
    "next-i18next": "^15.4.0",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.9.0",
    "react-dom": "^18.3.1",
    "recharts": "^3.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "swr": "^2.2.4",
    "tailwind-merge": "^3.4.0",
    "w3gjs": "^3.0.0",
    "@websites/infrastructure": "workspace:*",
    "@websites/ui": "workspace:*"
  },
  "devDependencies": {
    "@websites/prettier-config": "workspace:*",
    "@websites/test-utils": "workspace:*",
    "@next/bundle-analyzer": "^16.0.5",
    "@playwright/test": "^1.57.0",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18",
    "@websites/config-tailwind": "workspace:*",
    "@websites/eslint-config": "workspace:*",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "i18next-scanner": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mpq": "^0.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.0",
    "typescript": "^5.7.2",
    "wc3maptranslator": "^4.0.4"
  },
  "type": "module"
}
