{
  "name": "@websites/personalpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "fix:imports": "node scripts/addJsExtensions.js",
    "validate:translations": "node scripts/validateTranslations.js",
    "check:missing": "node scripts/checkMissingTranslations.js",
    "build:test": "tsc -p config/tsconfig.test.json && tsc-alias -p config/tsconfig.test.json && pnpm run fix-imports",
    "build:test:only": "tsc -p config/tsconfig.test.json && tsc-alias -p config/tsconfig.test.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules --max-old-space-size=4096 jest",
    "test:windows": "set NODE_OPTIONS=--experimental-vm-modules --max-old-space-size=4096 && jest",
    "test:math:ui": "cross-env NEXT_PUBLIC_TEST_MODE=true next dev -p 3001",
    "test:openai": "pnpm run build:test && node --experimental-vm-modules build/scripts/runner.js",
    "test:openai:count": "pnpm run build:test && node --experimental-vm-modules build/scripts/counter.js",
    "test:openai:results": "pnpm run build:test && node --experimental-vm-modules build/scripts/results.js",
    "test:openai:missing": "pnpm run build:test && node --experimental-vm-modules build/scripts/missing.js",
    "test:openai:failed": "pnpm run build:test && node --experimental-vm-modules build/scripts/failed.js",
    "test:openai:failed:10": "pnpm run build:test && node --experimental-vm-modules build/scripts/failed.js 10",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "migrate:weekly-to-classwork": "ts-node scripts/convertWeeklyAssessmentToClasswork.ts",
    "check:vercel": "tsx scripts/checkVercelDeployment.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env ANALYZE=true BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env ANALYZE=true BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@azure/msal-node": "^2.16.2",
    "@langchain/langgraph": "^0.2.63",
    "@langchain/openai": "^0.5.5",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@websites/config-tailwind": "workspace:*",
    "@websites/eslint-config": "workspace:*",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "docxtemplater": "^3.65.3",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "file-saver": "^2.0.5",
    "firebase-admin": "^13.0.1",
    "formidable": "^3.5.4",
    "glob": "^11.0.2",
    "googleapis": "^144.0.0",
    "html2pdf.js": "^0.12.1",
    "@websites/infrastructure": "workspace:*",
    "@websites/ui": "workspace:*",
    "jszip": "^3.10.1",
    "katex": "^0.16.22",
    "langchain": "^0.3.21",
    "mathjs": "^14.0.0",
    "module-alias": "^2.2.3",
    "next": "15.5.7",
    "nodemailer": "^6.9.16",
    "openai": "^6.9.1",
    "pizzip": "^3.2.0",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-katex": "^3.1.0",
    "react-tooltip": "^5.28.0",
    "recharts": "^3.3.0",
    "sharp": "^0.33.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "wc3maptranslator": "^4.0.4"
  },
  "devDependencies": {
    "@websites/prettier-config": "workspace:*",
    "@websites/test-utils": "workspace:*",
    "@next/bundle-analyzer": "^16.0.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chalk": "^0.4.31",
    "@types/dotenv": "^6.1.1",
    "@types/file-saver": "^2.0.7",
    "@types/formidable": "^3.4.6",
    "@types/html2pdf.js": "^0.10.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "^6.4.17",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.21",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^18",
    "@types/react-katex": "^3.0.4",
    "@types/react-tooltip": "^3.11.0",
    "@types/readline-sync": "^1.4.8",
    "@types/recharts": "^1.8.29",
    "@playwright/test": "^1.57.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "i18next-scanner": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "readline-sync": "^1.4.10",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.3",
    "typedoc": "^0.28.14",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "zod-to-json-schema": "^3.25.0"
  },
  "type": "module"
}
